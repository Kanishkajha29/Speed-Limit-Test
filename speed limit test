#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	   double A;
	   cin>>A;
	   double X;
	   cin>>X;
	   double c;
	   c=A/X;
	   double B, Y;
	   cin>>B>>Y;
	   double i;
	   i=B/Y;
	   if(c>i){
	       cout<<"Alice"<<endl;
	   }else if(i>c){
	       cout<<"Bob"<<endl;
	   }else{
	       cout<<"Equal"<<endl;
	   }
	  }
	
	return 0;
}
